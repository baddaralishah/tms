package com.badar.tms.repositories;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import com.badar.tms.models.TicketMedias;
import com.badar.tms.models.Tickets;

@Repository
public interface TicketMediaRepository  extends JpaRepository<TicketMedias, Long>{

	Tickets<TicketMedias> findByPostId(Long postId);
//    Optional<TicketMedias> findByIdAndPostId(Long id, Long postId);
}
