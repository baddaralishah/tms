package com.badar.tms.repositories;

import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import com.badar.tms.models.TStatuses;
import com.badar.tms.models.TicketMedias;
import com.badar.tms.models.Tickets;

@Repository
public interface TicketStatusRepository  extends JpaRepository<TStatuses, Long>{

	Tickets<TStatuses> findByTicketId(long ticketid);
    Optional<TStatuses> findByIdAndPostId(Long id, Long ticketId);
}
